{
  "presets": ["@babel/preset-react"],
  "plugins": [
    [
      "i18next-extract",
      {
        "outputPath": "public/locales/{{locale}}/{{ns}}.json",
        "locales": ["ar", "de", "en", "es", "fr", "it", "pt_br", "ru"],
        "discardOldKeys": true,
        "keySeparator": "::",
        "nsSeparator": ":::"
      }
    ]
  ]
}
